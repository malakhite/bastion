{
	"root": "libs/factbook",
	"sourceRoot": "libs/factbook/src",
	"projectType": "library",
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/factbook/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/factbook"],
			"options": {
				"jestConfig": "libs/factbook/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
